<div class="sin">
  <div class="col-md-8 col-lg-8" id="estatico">
   <div class="col-md-10 col-md-offset-1 tabMod">
      <div class="col-md-2 col-xs-2 tabModMenu">
        <ul>
        <li id="mod">Portal</li>
        <li id="mod">Noticias</li>
        <li id="mod">Usuarios</li>
        <li id="mod">Batallas</li>
        <li id="mod">Eventos y Torneos</li>
        </ul>
      </div>
      <div class="col-md-9 col-xs-9 col-md-offset-1 col-xs-offset-1 tabModFijo">
        <div class="col-md-12 col-xs-12 tabPortal">
          <img src="/images/cerrar.png" class="libre close img-responsive">
          <div class="col-md-5 col-xs-5 tabModUser">
            <div class="col-md-12 col-xs-12 tabTitulo">
              Miembros Recientes
            </div>
            <div class="col-md-12 col-xs-12 tabModCon letra">
              <% for(var i=0; i<us.length; i++) {%>
              <div class="col-md-12 col-xs-12">
              <div class="col-md-5 col-xs-5">
                <%=us[i].user_id %>
              </div> 
              <div class="col-md-7 col-xs-7">
                <%=us[i].mail %>
              </div>
              </div>
            <% } %>
            </div>
          </div>
          <div class="col-md-offset-1 col-xs-offset-1 col-md-5 col-xs-5 tabModET">
            <div class="col-md-12 col-xs-12 tabTitulo">
              Eventos y Torneos
            </div>
            <div class="col-md-12 col-xs-12 tabModCon">

            </div>
          </div>
          <div class="col-md-11 col-xs-11 tabModNot">
            <div class="col-md-12 col-xs-12 tabTitulo">
              Noticias y Anuncios
            </div>
            <div class="col-md-12 col-xs-12 tabModCon tabNotSimple">
            <% for(var i =0; i<n.length; i++){ %>
              <div class="col-md-12 col-xs-12">
                <div class="col-md-2 col-xs-3">
                  <%=n[i].fecha %>
                </div>
                <div class="col-md-10 col-xs-9">
                  <a href="Noticias/id/<%=n[i]['id'] %>"><%=n[i].titulo %></a>
                </div>
              </div>
            <% } %>
            </div>
          </div>
          <div class="col-md-11 col-xs-11 tabModEst">
            <div class="col-md-12 col-xs-12 tabTitulo">
              Estadisticas Generales
            </div>
            <div class="col-md-12 col-xs-12 tabModCon">
              <div class="col-md-12 col-xs-12">
                <div class="col-md-3 col-xs-3">
                 Usuarios Totales:
                </div>
                <div class="col-md-3 col-xs-3 EstUsers">
                  <%=usT %>
                </div>
                <div class="col-md-3 col-xs-3">
                  Personajes Totales:
                </div>
                <div class="col-md-3 col-xs-3 EstPjs">
                  <%=pj %>
                </div>
              </div>
              <div class="col-md-12 col-xs-12">
                <div class="col-md-3 col-xs-3">
                  Fecha de Inicio:
                </div>
                <div class="col-md-3 col-xs-3 EstFecha">
                  XX/XX/XX
                </div>
                <div class="col-md-3 col-xs-3">
                  Batallas Totales
                </div>
                <div class="col-md-3 col-xs-3 EstBatallas">
                  XXXXX
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>



  </div>
 

<div class="col-xs-12 col-md-4 col-lg-4 blanco">
  <div class="sin">
    <div class="col-xs-2 col-md-2" id="conectados">
      <ul id="ulConectados"></ul>
    </div>
    <div class="col-xs-10 col-md-10 tamano">
        <div class="col-md-10" id="mensajes_sistema">
          <div class="col-md-12 titChat1">
            <img src="/images/Mensaje_Sistema.png"></div>
          <div class="col-md-12 cuerpoChat1"></div>
        </div>
        <div class="col-md-10" id="mensajes_chat">
          <div class="col-md-12 titChat1"></div>
          <div class="col-md-12 cuerpoChat1">
            <ul id="my_ul"></ul>
          </div>
        </div>
        <div id="botones2" class="col-xs-11 col-md-10 col-md-offset-1 col-xs-offset-1">
          <% if(user) {%>
        <form id="chat_usuario" onsubmit="return false">
          <div class="col-md-8 col-xs-8">
            <input id="m" class="form-control">
          </div>
          <div class="xol-md-offset-1 col-xs-offset-1 col-md-3 col-xs-3">
            <button id="enviar" class="btn btn-primary btn-sm">Enviar</button>
          </div>
          </form>
          <% } else{%>
          <div class="vacio col-md-10 col-md-offset-1"></div>
          <% } %>
        </div>

    </div>
    </div>
  </div>
</div>